  },
  "scripts": {
    "start": "npm-run-all clean -p build:css references -p watch:*",
    "build": "npm-run-all clean -p build:css references -s build:eleventy",
    "build:eleventy": "eleventy",
    "watch:eleventy": "eleventy --serve",
    "build:css": "node css.js",
    "watch:css": "chokidar 'src/main.css' -c 'node css.js'",
    "clean": "rimraf src/_data/referencesByProject.json dist/",
    "references": "node projectReferences.js"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "autoprefixer": "^9.6.0",
    "chokidar-cli": "^1.2.2",
    "clean-css": "^4.2.1",
    "file-type": "^12.0.0",
    "jsdom": "^15.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.17",
    "rimraf": "^2.6.3"
  },
  "private": true,
  "browserslist": [
    "last 1 version",
    "> 1% in JP",
    "not dead"
  ]
